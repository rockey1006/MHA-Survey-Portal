<div id="<%= dom_id feedback %>">
  <p>
    <strong>Advisor:</strong>
    <%= feedback.advisor_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= feedback.category&.name || feedback.category_id %>
  </p>

  <p>
    <strong>Score:</strong>
    <%= feedback.score %>
  </p>

  <p>
    <strong>Survey Response:</strong>
    <%= feedback.surveyresponse_id %>
  </p>

  <p>
    <strong>Comments:</strong>
    <%= feedback.comments %>
  </p>

</div>
